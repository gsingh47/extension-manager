{
  "manifest_version": 3,
  "name": "Extension manager by Softech",
  "description": "Manage all your extensions in one place.",
  "version": "1.1.1",
  "action": {
    "default_popup": "popup.html",
    "default_title": "Open Extension Manager"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "management",
    "storage"
  ],
  "icons": {
    "16": "extension-manager.png",
    "32": "extension-manager.png",
    "48": "extension-manager.png",
    "128": "extension-manager.png"
  }
}
